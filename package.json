{
  "name": "easy-bookmark",
  "displayName": "Easy Bookmark",
  "version": "0.1.1",
  "description": "Easily manage your bookmarks.",
  "author": "shisongyan. <songyan_shi@163.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.20.0",
    "@ant-design/icons": "^5.3.6",
    "@plasmohq/storage": "^1.10.0",
    "antd": "^5.16.4",
    "mousetrap": "^1.6.5",
    "plasmo": "0.81.0",
    "radash": "^12.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "commands": {
      "create-bookmark": {
        "suggested_key": {
          "default": "Ctrl+D",
          "mac": "Command+D"
        },
        "description": "Create or edit bookmark"
      },
      "show-all-bookmarks": {
        "suggested_key": {
          "default": "Ctrl+K",
          "mac": "Command+K"
        },
        "description": "Display all bookmarks"
      }
    },
    "permissions": [
      "bookmarks",
      "contextMenus",
      "tabs"
    ],
    "host_permissions": [
      "https://*/*"
    ]
  }
}
