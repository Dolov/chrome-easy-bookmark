{
  "name": "easy-bookmark",
  "displayName": "Easy Bookmark",
  "version": "1.1.0",
  "description": "Easily manage your bookmarks.",
  "author": "shisongyan. <songyan_shi@163.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.20.0",
    "@ant-design/icons": "5.3.6",
    "@plasmohq/storage": "1.10.0",
    "antd": "5.16.4",
    "dayjs": "^1.11.13",
    "mousetrap": "1.6.5",
    "plasmo": "0.81.0",
    "radash": "12.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.3",
    "typescript": "5.3.3"
  },
  "manifest": {
    "commands": {
      "create-or-edit": {
        "suggested_key": {
          "default": "Ctrl+D",
          "mac": "Command+D"
        },
        "description": "新建 / 编辑"
      },
      "manage-or-search": {
        "suggested_key": {
          "default": "Ctrl+K",
          "mac": "Command+K"
        },
        "description": "管理 / 搜索"
      }
    },
    "web_accessible_resources": [
      {
        "resources": [
          "tabs/Settings.html"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "permissions": [
      "bookmarks",
      "contextMenus",
      "tabs",
      "identity"
    ],
    "host_permissions": [
      "https://*/*"
    ]
  }
}
